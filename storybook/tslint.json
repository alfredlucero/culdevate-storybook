{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "interface-name": [true, "never-prefix"],
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": false,
    "no-empty": false,
    "variable-name": false,
    "max-classes-per-file": false
  },
  "rulesDirectory": []
}